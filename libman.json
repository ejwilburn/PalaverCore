{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": 
  [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/"
    },
    {
      "library": "semantic-ui@2.2.10",
      "destination": "wwwroot/lib/semantic-ui/"
    },
    {
      "library": "jquery@3.2.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "cdnjs",
      "library": "mustache.js@2.3.0",
      "destination": "wwwroot/lib/mustache.js/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validation-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validate@1.17.0",
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-timeago@1.6.1",
      "destination": "wwwroot/lib/jquery-timeago/"
    },
    {
      "library": "blazy@1.8.2",
      "destination": "wwwroot/lib/blazy/"
    },
    {
      "library": "gifffer@1.5.0",
      "destination": "wwwroot/lib/gifffer/"
    }
  ]
}